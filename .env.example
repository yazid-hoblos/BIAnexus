# BIAnexus Environment Configuration
# Copy this to .env and add your actual API keys

# Server Configuration
PORT=3001

# AI API Configuration (Choose one)
# Option 1: OpenAI
OPENAI_API_KEY=sk-your-openai-key-here

# Option 2: Claude (Anthropic)
ANTHROPIC_API_KEY=your-anthropic-key-here

# API Selection (openai or anthropic)
AI_PROVIDER=mock

# Demo Mode (set to true to use mock responses without API calls)
DEMO_MODE=true
